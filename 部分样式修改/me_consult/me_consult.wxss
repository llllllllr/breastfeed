.container{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
  font-size: 28rpx;
  color: black;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.navigator{
  width: 94%;
  height: 40rpx;
  background-color: white;
  box-sizing: border-box;
  margin-top: 1rpx;
}
.n1,.n2,.n3{
  float: left;
  height: 100%;
  width: 33.33333333%;
  text-align: center;
}
.n1,.n3{
  border:2rpx solid black;
  box-sizing: border-box;
  border-radius: 10rpx 0rpx 0rpx 10rpx;
}
.n1{
  border-radius: 10rpx 0rpx 0rpx 10rpx;
}
.n3{
  border-radius: 0rpx 10rpx 10rpx 0rpx;
}
.n2{
  border-top:2rpx solid black;
  border-bottom:2rpx solid black;
  box-sizing: border-box;
}
.zixun{
  box-sizing: border-box;
  width:90%;
  height:220rpx;
  margin-top:15rpx;
  background-color: white;
  border-radius: 20rpx;
  border: 2rpx solid white;
}
.question,.doctor,.outcome,.time{
 
  width: 60%;
  margin-left: 30rpx;
  margin-top:16rpx;
}
.question{
  font-size: 32rpx;
}
.outcome{
  font-size:28rpx
}
.doctor{
  font-size: 26rpx
}
.time{
  font-size:22rpx;
}