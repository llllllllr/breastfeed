.ec-canvas {
  position: absolute;
  left: 0px;
  top: 0px;
  width: 95%;
  height: 800rpx;
  user-select: none;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  padding: 0px;
  margin: 0px;
  border-width: 0px;

}
 .canvas-style{
    position: relative;
    overflow: hidden;
    width: 100%;
    height: 1200rpx;
    padding: 0px;
    margin: 0px;
    border-width: 0px;
    cursor: default;
} 

